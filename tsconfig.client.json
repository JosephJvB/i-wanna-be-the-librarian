{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "dist-client",
    "noEmitOnError": true
  },
  "include": [
    "./client/**/*.vue",
    "./client/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ],
}
